{
  "name": "iquesters/user-management",
  "description": "A modular and extendable Laravel package that provides user management, role-based access control, and authentication features.",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^10.0|^11.0|^12.0",
    "spatie/laravel-permission": "^6.16",
    "laravel/socialite": "^5.16",
    "iquesters/masterdata": "^1.0",
    "iquesters/organisation": "^1.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "laravel/tinker": "^2.8"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Iquesters\\UserManagement\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iquesters\\UserManagement\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Pritam Bag",
      "email": "pritam02b@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Iquesters\\UserManagement\\UserManagementServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}